<h1>Demopage#index</h1>
<p>Find me in app/views/demopage/index.html.erb</p>

<% unless current_user %>
  <%= button_to "Sign in with Facebook", user_facebook_omniauth_authorize_path, method: :post %>
<% else %>
  <%= image_tag current_user[:image] %><br>
  <%= 'Name: ' + current_user[:name] %><br>
  <%= 'UID: ' + current_user[:uid] %><br>
  <%= 'Email: ' + current_user[:email] %><br>
  <%= link_to "Logout", destroy_user_session_path, :method => :delete %>
<% end %>
